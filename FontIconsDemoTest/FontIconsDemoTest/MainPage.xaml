<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:d="http://xamarin.com/schemas/2014/forms/design"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:fontValue="clr-namespace:FontIconsDemoTest.Fonts;assembly=FontIconsDemoTest"             
             mc:Ignorable="d"
             x:Class="FontIconsDemoTest.MainPage">
    <StackLayout>
        <!-- Place new controls here -->
        <Label 
            FontFamily="fontello"
           HorizontalOptions="Center"
               FontSize="Header"
                Text="{StaticResource IconCiro}"
           VerticalOptions="CenterAndExpand" />

        <Label 
            FontFamily="fontello"
           HorizontalOptions="Center"
               FontSize="Header"                
            Text="{x:Static fontValue:ConstanIconCiro.Map}"
           VerticalOptions="CenterAndExpand" />


        <Label 
            FontFamily="fontello"
           HorizontalOptions="Center"
               FontSize="Header"                
            Text="{x:Static fontValue:ConstanIconCiro.Warning}"
           VerticalOptions="CenterAndExpand" />
    </StackLayout>


</ContentPage>
